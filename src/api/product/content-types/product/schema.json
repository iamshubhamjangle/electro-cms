{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "currentPrice": {
      "type": "decimal",
      "default": 0,
      "min": 0,
      "max": 99999999
    },
    "subTitle": {
      "type": "string"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "sub_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-category.sub-category",
      "inversedBy": "products"
    },
    "traits": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::trait.trait",
      "inversedBy": "products"
    },
    "description": {
      "type": "text"
    },
    "originalPrice": {
      "type": "decimal"
    },
    "manufacturer": {
      "type": "string"
    },
    "rating": {
      "type": "decimal",
      "min": 0,
      "max": 5,
      "default": 0
    },
    "seller": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::seller.seller"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
